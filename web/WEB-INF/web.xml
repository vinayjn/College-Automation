<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlets.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PQuestionPapers</servlet-name>
        <servlet-class>servlets.PQuestionPapers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PFeeRecords</servlet-name>
        <servlet-class>servlets.PFeeRecords</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PBatch</servlet-name>
        <servlet-class>servlets.PBatch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PMstAttendance</servlet-name>
        <servlet-class>servlets.PMstAttendance</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PAssignment</servlet-name>
        <servlet-class>servlets.PAssignment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PMstQuestionPapers</servlet-name>
        <servlet-class>servlets.PMstQuestionPapers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PTimetable</servlet-name>
        <servlet-class>servlets.PTimetable</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PManageFaculty</servlet-name>
        <servlet-class>servlets.PManageFaculty</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PInvalidateBatch</servlet-name>
        <servlet-class>servlets.PInvalidateBatch</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PQuestionPapers</servlet-name>
        <url-pattern>/pquestionpapers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PFeeRecords</servlet-name>
        <url-pattern>/pfeerecords</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PBatch</servlet-name>
        <url-pattern>/pbatch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PMstAttendance</servlet-name>
        <url-pattern>/pmstattendance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PAssignment</servlet-name>
        <url-pattern>/passignment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PMstQuestionPapers</servlet-name>
        <url-pattern>/pmstquestionpapers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PTimetable</servlet-name>
        <url-pattern>/ptimetable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PManageFaculty</servlet-name>
        <url-pattern>/pmanagefaculty</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PInvalidateBatch</servlet-name>
        <url-pattern>/pinvalidatebatch</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
